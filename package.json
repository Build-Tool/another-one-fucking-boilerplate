{
  "private": true,
  "name": "Boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate",
  "scripts": {
    "lint": "eslint webpack.config.js src/ && csscomb -l -v src/",
    "fix": "eslint --fix webpack.config.js src/ && csscomb src/",
    "build": "npm run lint && rm -rf build/ && node --harmony node_modules/.bin/webpack"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "cssnext-loader": "^1.0.1",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "react": "^0.13.3",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2"
  }
}
